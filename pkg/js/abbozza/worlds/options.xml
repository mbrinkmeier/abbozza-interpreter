<?xml version="1.0" encoding="UTF-8"?>
<options>
    <group name="gui.tools">
        <item name="gui.debug" option="debug" default="false"/>
        <item name="gui.source" option="source" default="false"/>
        <item name="gui.calls" option="calls" default="false"/>
    </group>
    <group name="gui.blocks">
        <item name="gui.advanced" option="advanced" default="false">
        </item>
        <item name="gui.operations" option="operations" default="false">
            <item name="gui.local_variables" option="localVars" default="false"/>
        </item>
        <group name="gui.datastructures">
            <item name="gui.stacks" option="stack" default="false"/>
            <item name="gui.queues" option="queue" default="false"/>
            <item name="gui.lists" option="list" default="false"/>
            <item name="gui.bintrees" option="bintree" default="false"/>
        </group>
        <group name="gui.arrays">
            <choice name="gui.no_arrays" option="noArrays" default="true"/> 
            <choice name="gui.lin_arrays" option="linArrays" default="false"/> 
            <choice name="gui.mult_arrays" option="multArrays" default="false"/> 
        </group> 
        <!-- item name="gui.teacher" option="teacher"/-->
    </group>
    <group name="gui.worlds"></group>
    <group name="gui.misc">
        <group name="gui.colorize">
            <choice name="gui.color_default" option ="colorDefault" default="true"/>
            <choice name="gui.color_category" option ="colorCategory" default="false"/>
            <choice name="gui.color_type" option ="colorType" default="false"/>
        </group>
    </group>
</options>
